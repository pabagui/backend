<h1>Productos</h1>
{{#if hayProductos}}
    <table>
        <tr>
        <th>Nombre productos</th>
        <th>Precio</th>
        <th>Foto</th>
        </tr>
        {{#each productos}}
        <tr>
            <td>{{this.title}} </td>
            <td>{{this.price}} </td>
            <td><img src={{this.thumbnail}} style='width:70px;height:90px;'></td>
        </tr>
        {{/each}}
    </table>
    {{else}}
    <h3>No hay productos</h3>
{{/if}}